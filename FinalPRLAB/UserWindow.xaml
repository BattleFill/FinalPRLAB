<Window x:Class="FinalPRLAB.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalPRLAB"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="820">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="180*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Armor" Grid.Column="0" Grid.Row="0" Click="Armor_Click"/>
        <Button Content="Donate" Grid.Column="0" Grid.Row="1" Click="Donate_Click"/>
        <Button Content="Fraction" Grid.Column="0" Grid.Row="2" Click="Fraction_Click"/>
        <Button Content="Guns" Grid.Column="0" Grid.Row="3" Click="Guns_Click"/>
        <Frame x:Name="fr" Grid.Row="0" Grid.Column="1" Grid.RowSpan="4" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
